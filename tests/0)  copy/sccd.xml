<?xml version="1.1" ?>
<diagram author="Sam Pieters" name="GlobalData">
    <description>
        Test X: check if global data can be changed
    </description>
    <top>
        GLOBAL_INT = 0
    </top>
    <inport name="ui"/>
    <outport name="ui"/>
    <class name="MainApp" default="true">
        <scxml initial="state1">
            <state id="state1">
                <onentry>
                    <script>
                        print(GLOBAL_INT)
                    </script>
                </onentry>
            </state>
        </scxml>
    </class>
</diagram>