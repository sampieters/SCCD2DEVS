<?xml version="1.1" ?>
<diagram author="Sam Pieters" name="GlobalOutputTest">
    <description>
        Check if the model raises an output.
    </description>
    <outport name="Outport"/>
    <class name="MainApp" default="true">
        <scxml initial="state1">
            <state id="state1">
                <onentry>
                    <raise port="Outport" event="output_event" />
                </onentry>
            </state>
        </scxml>
    </class>
</diagram>