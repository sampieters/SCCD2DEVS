<?xml version="1.1" ?>
<diagram author="Sam Pieters" name="AfterTransitionTest">
    <description>
        Test 2: Check if a statechart transitions after a certain time.
    </description>
    <class name="MainApp" default="true">
        <scxml initial="state1">
            <parallel id="state1">
                <state id="state11">
                    <state id="state11">
                        <transition target="." after="0.05" />
                    </state>
                </state>
                <state id="state12">
                    <state id="state12">
                        <transition target="." after="0.05" />
                    </state>
                </state>
                <transition target="../end_parallel" />
            </parallel>
            <state id="end_parallel" />
        </scxml>
    </class>
</diagram>